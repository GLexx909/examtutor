  .container
    .row
      .col-md-12.text-center
        .section-title
          h2
            | Contact Us
    .row
      .col-md-6
        .contact-form
          form#ajax-contact[action="mailer.php" method="post"]
            .form-group.in_name
              input#name.form-control[type="text" name="name" placeholder="Name" required="required"]
            .form-group.in_email
              input#email.form-control[type="email" name="email" placeholder="Email" required="required"]
            .form-group.in_message
              textarea#message.form-control[rows="5" name="message" placeholder="Message" required="required"]
            .actions
